{
  "name": "subkiosk",
  "version": "0.0.1",
  "description": "Self-Order DEMO Kiosk using React Native and MobX.",
  "main": "index.js",
  "dependencies": {
    "accounting-js": "^1.1.1",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "mobx-state-tree": "^3.14.0",
    "react": "^16.8.6",
    "react-native": "^0.59.8",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^3.11.0",
    "react-spring": "^8.0.20",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "standard": "^12.0.1"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  },
  "scripts": {
    "build": "rm -rf ./build && mkdir ./build && react-native bundle --assets-dest ./build --bundle-output ./build/main.jsbundle --dev false --entry-file ./index.js --platform ios",
    "clean": "rm -f ./yarn.lock && rm -f ./yarn-*.log && yarn cache clean",
    "cli": "node node_modules/react-native/local-cli/cli.js start",
    "devices": "instruments -s devices",
    "init": "rm -rf ./node_modules && yarn",
    "lint": "standard --fix --parser babel-eslint",
    "nuke": "yarn run clean && yarn run init",
    "start": "concurrently 'yarn run cli' 'react-native run-ios --simulator=\"iPad Pro (12.9-inch) (2nd generation) (12.2)\"'",
    "test": "standard --parser babel-eslint",
    "upgrade": "npx upgradeps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lropero/subkiosk.git"
  },
  "author": "Luciano Ropero <lropero@gmail.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lropero/subkiosk#readme"
}
