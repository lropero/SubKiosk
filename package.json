{
  "name": "SubKiosk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -f ./yarn.lock && rm -f ./yarn-*.log && yarn cache clean",
    "cli": "node node_modules/react-native/local-cli/cli.js start",
    "init": "rm -rf ./node_modules && yarn",
    "lint": "standard --fix --parser babel-eslint",
    "nuke": "yarn run clean && yarn run init",
    "start": "concurrently 'yarn run cli' 'react-native run-ios --simulator=\"iPad Pro (10.5-inch) (12.1)\"'",
    "test": "standard --parser babel-eslint",
    "upgrade": "node upgrade.js"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-native": "^0.59.1",
    "react-native-gesture-handler": "^1.1.0",
    "react-navigation": "^3.5.1",
    "react-spring": "^8.0.17",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/runtime": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "standard": "^12.0.1"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
